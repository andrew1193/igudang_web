    <script>
        $("#btnSubmit3").click(function () {
            var name = $('#txtFullName').val();
            var emel = $('#txtEmail3').val();
            var orderNum = $('#txtOrderNum').val();
            var typeOfEnquiry = $('#ddlTypeOfEnquiry').val();
            var message = $('#txtMessage').val();

            if (name == null || name == "") {
                alert("Please insert a name!");
                return false;
            }

            if (emel == null || emel == "") {
                alert("Please insert a email address!");
                return false;
            }

            var regex = /^(([^<>()\[\]\\.,;:\s@@"]+(\.[^<>()\[\]\\.,;:\s@@"]+)*)|(".+"))@@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            if (!regex.test(emel)) {
                alert("Please insert a valid email address!");
                return false;
            }

            if (typeOfEnquiry == "") {
                alert("Please select a type of enquiry!");
                return false;
            }

            if (message == null || message == "") {
                alert("Please leave a message!");
                return false;
            }

            $.post("/Landing/FormSubmit3", { name: name, email: emel, order: orderNum, type: typeOfEnquiry, msg: message }, function (data, status) {
                if (data.success) {
                    alert(data.msg);
                    location.reload(true);
                }
                else {
                    alert(data.msg);
                }
            });
        });


