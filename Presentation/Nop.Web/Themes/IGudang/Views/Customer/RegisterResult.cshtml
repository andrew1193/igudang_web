@model RegisterResultModel
@using Nop.Web.Models.Customer;
@{
    Layout = "~/Views/Shared/_ColumnsOne.cshtml";

    //title
    Html.AddTitleParts(T("PageTitle.Register").Text);
    //page class
    Html.AppendPageCssClassParts("html-registration-result-page");
}


@using (Html.BeginForm(new { ReturnUrl = Request.QueryString["ReturnUrl"] }))
{
    <div class="content-wrap pt-2">
        <div class="container clearfix">
            <div class="accordion accordion-lg divcenter nobottommargin clearfix" style="max-width: 140px;">
                <div class="page registration-result-page">
                    <div class="page-title">
                        <h1>@T("Account.Register")</h1>
                    </div>
                    <div class="page-body">
                        <div class="result" style="width:500px">
                            @Model.Result

                            <br/><br />
                        </div>
                        <div class="buttons">
                            <input type="submit" name="register-continue" class="button button-3d button-black nomargin" value="@T("Account.Register.Result.Continue")" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}




